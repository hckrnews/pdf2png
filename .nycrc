{
    "all": true,
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "**/*.test.js"
    ],
    "reporter": [
        "text",
        "text-summary",
        "lcov",
        "clover"
    ],
    "check-coverage": true,
    "watermarks": {
        "lines": [
            75,
            90
        ],
        "functions": [
            75,
            90
        ],
        "branches": [
            75,
            90
        ],
        "statements": [
            75,
            90
        ]
    },
    "branches": 75,
    "lines": 75,
    "functions": 75,
    "statements": 75
}
